{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/charge",
      "dest": "/api/charge.py"
    },
    {
      "src": "/api/callback",
      "dest": "/api/callback.py"
    },
    {
      "src": "/api/search-user",
      "dest": "/api/search-user.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
